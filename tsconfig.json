{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitThis": true,
    "strictNullChecks": false,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "module": "commonjs",
    "target": "es5",
    "allowJs": true
  },
  "include": [
    "src/**/*",
    "node_modules/phaser-ce/typescript/phaser.d.ts"
  ]
}